{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off",
        "input-image-alt": "off"
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off",
        "duplicate-id-aria": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "select-name": "off",
        "label": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "meta-viewport": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-moz-box-align"
        ]
      }
    ],
    "axe/structure": [
      "default",
      {
        "listitem": "off"
      }
    ],
    "no-inline-styles": "off"
  }
}